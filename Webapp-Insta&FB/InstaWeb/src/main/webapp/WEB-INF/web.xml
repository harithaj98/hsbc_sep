<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>InstaRegistrationServlet</servlet-name>
  	<display-name>InstaRegistrationServlet</display-name>
  	<description></description>
  	<servlet-class>com.instaweb.controller.InstaRegistrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InstaLoginServlet</servlet-name>
  	<display-name>InstaLoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.instaweb.controller.InstaLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>InstaRegistrationServlet</servlet-name>
  	<url-pattern>/InstaRegistrationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InstaLoginServlet</servlet-name>
  	<url-pattern>/InstaLoginServlet</url-pattern>
  </servlet-mapping>
</web-app>
